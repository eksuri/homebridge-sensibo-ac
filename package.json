{
  "name": "homebridge-sensibo-ac",
  "description": "Homebridge plugin for Sensibo - Smart AC Control",
  "version": "2.4.0-alpha.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/nitaybz/homebridge-sensibo-ac.git"
  },
  "license": "GNU",
  "preferGlobal": true,
  "keywords": [
    "homebridge-plugin",
    "homebridge-sensibo-ac",
    "homebridge-sensibo",
    "sensibo",
    "sensibo-sky",
    "sensibo-air",
    "sensibo-airq",
    "sensibo-air-plus",
    "sensibo-ac",
    "smart-ac",
    "category-climate"
  ],
  "bugs": {
    "url": "https://github.com/nitaybz/homebridge-sensibo-ac/issues"
  },
  "engines": {
    "node": ">=10.17.0",
    "homebridge": ">=0.4.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "fakegato-history": "^0.5.6",
    "node-persist": "^3.0.5",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "nodemon": "^2.0.20"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "."
    ],
    "ext": "js",
    "ignore": [
      "node_modules"
    ],
    "exec": "npx homebridge -I -D",
    "signal": "SIGTERM",
    "env": {
      "NODE_OPTIONS": "--trace-warnings"
    }
  },
  "funding": [
    {
      "type": "paypal",
      "url": "https://paypal.me/nitaybz"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/nitaybz"
    },
    {
      "type": "kofi",
      "url": "https://ko-fi.com/nitaybz"
    }
  ]
}
